<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.Toolbar xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/toolbar"
    android:layout_width="match_parent"
    android:layout_height="56dp"
    android:background="@color/colorTopBar"
    android:clipToPadding="false"
    android:contentInsetEnd="0dp"
    app:contentInsetLeft="0dp"
    app:contentInsetRight="0dp"
    app:contentInsetStart="0dp"
    app:layout_collapseMode="pin">
    <!--app:layout_collapseMode="1"-->
    <!--android:layout_height="UNKNOWN_DATA_0x3801"-->


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="55dp"
        android:gravity="top|bottom|left|right|center_vertical|fill_vertical|center_horizontal|fill_horizontal|center|fill|start|end"
        android:paddingLeft="10dp"
        android:paddingRight="10dp">
        <!--app:layout_height="UNKNOWN_DATA_0x3701"-->


        <LinearLayout
            android:id="@+id/lytFirstMenuButton"
            android:layout_width="35dp"
            android:layout_height="56dp"
            android:paddingBottom="10dp"
            android:paddingLeft="4dp"
            android:paddingRight="4dp"
            android:paddingTop="10dp">
            <!--android:layout_width="UNKNOWN_DATA_0x2201"-->
            <!--android:layout_height="UNKNOWN_DATA_0x3801"-->

            <ImageView
                android:id="@+id/imgFirstMenuButton"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:padding="3dp"
                android:scaleType="centerInside" />


        </LinearLayout>


        <LinearLayout
            android:id="@+id/lytSecondMenuButton"
            android:layout_width="35dp"
            android:layout_height="56dp"
            android:paddingBottom="10dp"
            android:paddingLeft="4dp"
            android:paddingRight="4dp"
            android:paddingTop="10dp">

            <!--android:layout_width="UNKNOWN_DATA_0x2201"-->
            <!--android:layout_height="UNKNOWN_DATA_0x3801"-->

            <ImageView
                android:id="@+id/imgSecondMenuButton"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:padding="3dp"
                android:scaleType="centerInside" />

        </LinearLayout>


        <TextView
            android:id="@+id/txtTitle"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_margin="4dp"
            android:layout_weight="1.0"
            android:gravity="center_vertical|center_horizontal|center"
            android:lines="1"
            android:maxLines="1"
            android:singleLine="true"
            android:text="سوپرمارکت های نزدیک من"
            android:textColor="@color/textColorDarkToolbar"
            android:textSize="@dimen/textSizeToolbar" />
        <!--<com.hollowsoft.library.fontdroid.TextView-->
        <!--style="@style/iranSansNormal"-->
        <!--app:ellipsize="end"-->
        <!--android:layout_width="UNKNOWN_DATA_0x1"-->


        <ImageView
            android:id="@+id/imgFakeButton"
            android:layout_width="30dp"
            android:layout_height="56dp" />
        <!--android:layout_height="UNKNOWN_DATA_0x3801"-->
        <!--android:layout_width="UNKNOWN_DATA_0x1e01"-->


        <LinearLayout
            android:id="@+id/lytRightButton"
            android:layout_height="56dp"
            android:layout_width="34dp"
            android:paddingBottom="10dp"
            android:paddingLeft="4dp"
            android:paddingRight="4dp"
            android:paddingTop="10dp">
            <!--android:layout_height="UNKNOWN_DATA_0x3801"-->
            <!--android:layout_width="UNKNOWN_DATA_0x2201"-->

            <ImageView
                android:contentDescription="go to previous page"
                android:id="@+id/imgRightButton"
                android:layout_height="match_parent"
                android:layout_width="match_parent"
                android:padding="3dp"
                android:src="@drawable/ic_back_dark" />
            <!--android:src="@drawable/ic_back_dark"-->

        </LinearLayout>


    </LinearLayout>

</android.support.v7.widget.Toolbar>

